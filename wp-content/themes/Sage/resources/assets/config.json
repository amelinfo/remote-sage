{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ],
    "style": [
      "./scripts/style.js"
    ],
    "slick": [
      "./scripts/slick.js",
      "./scripts/slick.css"
    ]
  },
  "publicPath": "/wp-content/themes/sage",
  "devUrl": "http://localhost/new_theme",
